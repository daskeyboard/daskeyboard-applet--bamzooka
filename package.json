{
  "name": "daskeyboard-applet--bamzooka",
  "version": "1.0.0",
  "displayName": "Bamzooka",
  "description": "Automate your workflow with reusable checklists. Get a notification when you have assignments to complete",
  "longDescription": "Get a visual notification on your Das Keyboard Q series when there is new assignments in your Bamzooka workspace",
  "officialProductName": "Bamzooka",
  "author": "Bamzooka",
  "appUrl": "https://bamzooka.com",
  "license": "MIT",
  "icon": "assets/icon.png",
  "image": "assets/image-missing.png",
  "authorName": "Bamzooka",
  "authorUrl": "https://bamzooka.com",
  "issuesUrl": "https://github.com/daskeyboard/daskeyboard-applet--bamzooka/issues",
  "homePageUrl": "https://bamzooka.com",
  "developerRepoUrl": "https://github.com/metadot/q-applet-github",
  "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.txt",
  "publisher": "Bamzooka",
  "changelogUrl": "CHANGELOG.md",
  "readMeUrl": "README.md",
  "readMeEndUserUrl": "README_END_USER.md",
  "isSingleton": false,
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "engines": {
    "das-keyboard-q": "3.2.6"
  },
  "qConfig": {
    "geometry": {
      "width": 1,
      "height": 1
    },
    "authorization": {
      "type": "apiKey",
      "hint": "Get your api key from your profile in the Bamzooka application",
      "supportUrl": "http://localhost:4200/workspaces?openUserProfile=true"
    },
    "questions": [
      {
        "key": "workspaceId",
        "label": "Choose a workspace to get assignments from",
        "help": "Select a workspace from the list",
        "required": true,
        "order": 1,
        "controlType": "dropdown",
        "dynamic": true,
        "options": []
      }
    ]
  },
  "dependencies": {
    "daskeyboard-applet": "^2.11.4",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "mocha": "^6.2.2"
  }
}